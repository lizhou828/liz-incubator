    <!-- public remote repositories -->
    <repositories>
        <repository>
            <id>liz-release</id>
            <name>nexus</name>
            <url>http://localhost:8081/nexus/content/repositories/liz-release/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>liz-snapshot</id>
            <name>liz-snapshot</name>
            <url>http://localhost:8081/nexus/content/repositories/liz-snapshot/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
