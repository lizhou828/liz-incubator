            </snapshots>
        </repository>
        <repository>
            <id>SonaType</id>
            <name>SonaType Repository</name>
            <url>http://repository.sonatype.org/content/groups/public/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>Glassfish</id>
            <name>Glassfish Repository</name>
            <url>http://maven.glassfish.org/content/groups/glassfish/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
